
@{
    ViewBag.Title = "Vaccine";
}

<div class="hero-wrap hero-bread" style="background-image: url('../../Front_Files/images/Vaccine_Banner.jpg');">
    <div class="container">
        <div class="row no-gutters slider-text align-items-center justify-content-center">
            <div class="col-md-9 ftco-animate text-center">
                <h1 class="mb-0 bread">Medication Catalog</h1>
            </div>
        </div>
    </div>
</div>


