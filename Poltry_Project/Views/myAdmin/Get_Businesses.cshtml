@model IEnumerable<Poltry_Project.Poultry_Hub_SR.User>

@{
    ViewBag.Title = "Businesses";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

@if (Model.Count() > 0)
{
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">

                <div class="card">
                    <div class="card-body">
                        <div class="table-responsive">
                            <table id="zero_config" class="table table-striped table-bordered">
                                <thead>
                                    <tr>
                                        <th>Supplier</th>
                                        <th>Email</th>
                                        <th>Cell Phone Numbers</th>
                                        <th>More</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    @foreach (var i in Model)
                                    {
                                        <tr>
                                            <td>@i.First_Name @i.Last_Name</td>
                                            <td>@i.Email</td>
                                            <td>@i.Contacts</td>
                                            <td><a href="~/myAdmin/Get_Business_Details/@i.Id"> More <i class="mdi mdi-arrow-right"></i> </a></td>

                                        </tr>
                                    }
                                </tbody>

                            </table>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
}
else
{


    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <h1 class="text-dark text-center">There are no registered businesses currently...</h1>
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />
                <br />

            </div>
        </div>
    </div>
}

